{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [
        "build/**",
        ".next/**",
        "!.next/cache/**",
        "src/codegen/**",
        "scripts/generated/**",
        "config/generated/**"
      ],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "lint:fix": {},
    "dev": { "persistent": true },
    "start": { "dependsOn": ["build"] },
    "clean": { "cache": false },
    "test": { "dependsOn": ["^build"] },
    "analyze": {}
  }
}
