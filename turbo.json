{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "outputs": [
        "build/**",
        ".next/**",
        "!.next/cache/**",
        "src/codegen/**",
        "scripts/generated/**",
        "config/generated/**",
        "public/tokens/generated/**",
        "tsconfig.tsbuildinfo"
      ],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "lint:fix": {},
    "dev": {
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "test:e2e": {
      "dependsOn": ["^build"]
    },
    "analyze": {}
  }
}
