{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["build/**", ".next/**", "!.next/cache/**"],
      "dependsOn": ["^generate", "^build"]
    },
    "lint": {},
    "lint:fix": {},
    "dev": { "dependsOn": ["build"] },
    "start": { "dependsOn": ["build"] },
    "clean": { "cache": false },
    "test": { "dependsOn": ["build"] },
    "analyze": {},
    "generate": {
      "outputs": [
        "src/codegen/**",
        "config/wallet-registry.ts",
        "config/wallet-assets.ts",
        "config/chain-infos.ts"
      ],
      "dependsOn": ["^generate"]
    }
  }
}
