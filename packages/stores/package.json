    "dayjs": "^1.10.7",
    "debounce": "^1.2.1",
    "deepmerge": "^4.2.2",
    "long": "^5.2.3",
    "mobx": "^6.3.10",
    "mobx-utils": "^6.0.4",
    "protobufjs": "^6.11.2",
    "sha.js": "^2.4.11",
    "utility-types": "^3.10.0",
    "@osmosis-labs/types": "^1.0.0",
    "@osmosis-labs/utils": "^1.0.0"
  },
  "resolutions": {
    "@keplr-wallet/stores": "file:../keplr-stores",
    "@osmosis-labs/keplr-hooks": "file:../keplr-hooks"
  }
