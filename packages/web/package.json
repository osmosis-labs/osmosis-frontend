  "scripts": {
    "predev": "yarn generate",
    "dev": "concurrently --raw --kill-others 'next dev' 'yarn watch'",
    "clean": "rm -rf node_modules; rm -rf .next; rm -rf config/generated",
    "build": "yarn generate && next build",
    "test": "yarn generate && dotenv -c -- jest --passWithNoTests",
    "analyze": "dotenv -v ANALYZE=true -- next build",
    "start": "next start",
    "lint": "prettier --check \"**/*\" && next lint",
    "lint:fix": "prettier --write \"**/*\" && next lint --fix",
    "pre-commit": "lint-staged",
    "generate": "yarn generate:assets && yarn generate:cosmos-kit-wallet-list && yarn generate:sprite-ids",
    "generate:assets": "dotenv -c -- tsx config/asset-list/generate-asset-lists.ts",
    "generate:cosmos-kit-wallet-list": "dotenv -c -- tsx config/generate-cosmos-kit-wallet-list.ts",
    "generate:sprite-ids": "dotenv -c -- tsx config/generate-sprite-ids.ts",
    "watch": "yarn watch:sprite-ids",
    "watch:sprite-ids": "chokidar 'public/icons/sprite.svg' -c 'yarn generate:sprite-ids'"
  },
