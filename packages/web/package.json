    "clean": "rm -rf node_modules; rm -rf .next; rm config/chain-infos.ts",
    "build": "yarn generate && next build",
    "analyze": "dotenv -v ANALYZE=true -- next build",
    "test": "jest --watch",
    "start": "next start",
    "lint": "prettier --check \"**/*\" && next lint",
    "lint:fix": "prettier --write \"**/*\" && next lint --fix",
    "pre-commit": "lint-staged",
    "generate": "yarn generate:chain-info && yarn generate:wallet-assets && yarn generate:wallet-registry",
    "generate:chain-info": "ts-node --esm config/generate-chain-infos/index.ts",
    "generate:wallet-assets": "ts-node --esm config/generate-wallet-assets/index.ts",
